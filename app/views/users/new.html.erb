<h1>Sign up</h1>
<div class="form-group">
  <%= simple_form_for @user do |f| %>
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :email %>
      <%= f.input :password %>
      <%= f.input :password_confirmation %>
      <%= f.submit "Sign Up", class:"btn btn-primary" %>
  <% end %>
</div>
